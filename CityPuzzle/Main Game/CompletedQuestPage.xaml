<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="CityPuzzle.CompletedQuestPage">
    <ContentPage.Content>
        <StackLayout>
            <Frame BackgroundColor="#F3BD21" Padding="24" CornerRadius="10">
                <Label x:Name="completedName" HorizontalOptions="CenterAndExpand" FontSize="36" TextColor="Black" ></Label>
            </Frame>
                <Image x:Name="completedImg" WidthRequest="300" HeightRequest="300"></Image>
            <Frame BackgroundColor="#F3BD21" Padding="24" CornerRadius="10">
                <Label x:Name="completedInfo" HorizontalOptions="CenterAndExpand" FontSize="18" TextColor="Black" ></Label>
            </Frame>
            <Button Text="Pradėti naują užduotį" Clicked="NewPuzzle_clicked"></Button>

        </StackLayout>
    </ContentPage.Content>
</ContentPage>