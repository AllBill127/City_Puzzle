<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="CityPuzzle.CompliteCreating"
             BackgroundImageSource="logos.png" WidthRequest="600" HeightRequest="200">
    <StackLayout>
        <Label x:Name="idplace" FontSize="30" TextColor="Black" VerticalTextAlignment="Center" HorizontalTextAlignment="Center" ></Label>

        <Picker x:Name="picker" Title="Select size" SelectedIndex="0" />

        <ListView x:Name="MyListView"
           
            ItemTapped="Handle_ItemTapped"
            CachingStrategy="RecycleElement">
        </ListView>
        <Button Text="Paleisti" Clicked="Save_Click"></Button>
    </StackLayout>
</ContentPage>
